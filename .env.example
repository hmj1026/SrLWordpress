# 資料庫設定
MYSQL_ROOT_PASSWORD=
MYSQL_DATABASE=
MYSQL_USER=
MYSQL_PASSWORD=

# Nginx 設定
PROJECT_NAME=example_project    # 專案名稱，用於 SSL 證書命名
NGINX_HOST=develop.dev         # 開發環境預設域名
ENV_TYPE=dev                   # 環境類型：dev 或 prod

# PHP 設定
PHP_MEMORY_LIMIT=256M
MAX_EXECUTION_TIME=120
UPLOAD_MAX_FILESIZE=32M
POST_MAX_SIZE=32M
PHP_DISPLAY_ERRORS=Off
PHP_DISPLAY_STARTUP_ERRORS=Off
PHP_TIMEZONE=Asia/Taipei

# MySQL 連接限制
MYSQL_MAX_CONNECTIONS=50
